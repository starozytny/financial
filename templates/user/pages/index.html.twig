{% extends 'user/pages/template.html.twig' %}

{% set page_title %}Dashboard{% endset %}
{% set submenuActive = "Planning" %}

{% block main_content %}
    <div id="planning"
         data-donnees="{{ donnees }}"
         data-categories="{{ categories }}"
         data-year="{{ year }}"
         data-total-init="{{ totalInit }}"
         data-have-cashback="{{ app.user.userIdentifier == "shanbo" }}"
    >
        {% include "dashboard/layout/components/spinner.html.twig" %}
    </div>
{% endblock %}
